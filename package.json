    "ts-loader": "9.2.7",
    "ts-node": "10.6.0",
    "tslib": "2.3.1",
    "tslint": "5.20.1",
    "typescript": "4.6.2",
    "webpack-dev-server": "4.15.1",
    "zone.js": "0.13.1"
  },
  "peerDependencies": {
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/core": "*",
