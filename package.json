{
  "name": "cypress-angular-unit-test",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.lib.json",
    "watch": "tsc --project tsconfig.lib.json -w",
    "semantic-release": "semantic-release",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "ng": "ng",
    "app-start": "ng serve",
    "app-build": "ng build",
    "docker:run": "docker run -it -v ${PWD}:/e2e -w /e2e -e CYPRESS_updateSnapshots cypress/included:5.1.0"
  },
  "private": false,
  "files": [
    "dist",
    "support.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "dist/index.js",
  "types": "dist",
  "homepage": "https://github.com/bahmutov/cypress-angular-unit-test#readme",
  "dependencies": {
    "debug": "4.3.1"
  },
  "devDependencies": {
    "@angular/animations": "11.2.7",
    "@angular/cdk": "11.2.6",
    "@angular/common": "11.2.7",
    "@angular/core": "11.2.7",
    "@angular/compiler": "11.2.7",
    "@angular/forms": "11.2.7",
    "@angular/material": "11.2.6",
    "@angular/platform-browser": "11.2.7",
    "@angular/platform-browser-dynamic": "11.2.7",
    "@angular/router": "11.2.7",
    "@angular-devkit/build-angular": "0.1102.6",
    "@angular/cli": "11.2.6",
    "@angular/compiler-cli": "11.2.7",
    "@angular/language-service": "11.2.7",
    "@cypress/code-coverage": "3.9.4",
    "@cypress/webpack-preprocessor": "5.6.0",
    "@types/cypress-image-snapshot": "3.1.5",
    "@types/node": "8.10.66",
    "angular-router-loader": "0.8.5",
    "angular2-template-loader": "0.6.2",
    "codelyzer": "6.0.1",
    "cypress": "6.8.0",
    "cypress-angular-unit-test": "file:.",
    "cypress-image-snapshot": "4.0.1",
    "html-loader": "2.1.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "ngx-build-plus": "11.0.0",
    "raw-loader": "1.0.0",
    "to-string-loader": "1.1.6",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "tslint": "5.20.1",
    "husky": "4.3.8",
    "ng-inline-svg": "12.1.0",
    "typescript": "4.2.3",
    "zone.js": "0.11.4",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "primeng": "11.3.1",
    "rxjs": "6.6.7",
    "tslib": "2.1.0",
    "semantic-release": "17.4.2",
    "core-js": "3.9.1"
  },
  "peerDependencies": {
    "@angular/common": "*",
    "@angular/core": "*",
    "@angular/compiler": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "zone.js": "*",
    "core-js": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-angular-unit-test.git"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "break",
          "release": "major"
        }
      ]
    }
  }
}
